}

class WordPair {
    - word
    - url
    +WordPair(word: String,url:String)
    +WordPair()
    +checkURL(): boolean
    + toString():String


}

class Gui {
    + drawGUI()

}
interface PersistanceStrategy{
    +save(): void
    +load(): void
}
class SaveJson{
    +save(): void
    +load(): void
}

file file{

}

